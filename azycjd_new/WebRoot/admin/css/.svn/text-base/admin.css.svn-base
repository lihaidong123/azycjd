.main_div{ width:1100px; margin:0 auto;}
.admin_top_right{ color:#FFFFFF; font-size:16px; font-family:"黑体";}
.admin_top_right div{ float:left; line-height:40px; margin-left:15px;}
.menu_items div{ line-height:45px; font-size:14px; margin-top:11px; padding-left:10px; cursor:pointer;}
.menu_items div:hover{ background-color:#a0d1e9; color:#292a2b;}

.menu_items_div_click{background-color:#1e9bd6; color:#ffffff;}

.top_menu{font-size:16px; font-family:"黑体";}
.top_menu div{ width:105px; height:35px; color:#ffffff; float:left; padding-top:65px; text-align:center;}
.top_menu div:hover{ background-color:#FFFFFF; cursor:pointer;}


.tableCss{margin-top:5px; font-family: '黑体'; }
.tableCss table{ border-collapse:collapse;}
.tableCss tr{ line-height:35px;}
.tableCss td{border:#c4d1da 1px solid;font-size: 13px; padding-left:10px; font-weight: normal;}
.titleCss{ font-weight:bold; color:#525556; background-color:#d8e3e9 !important;line-height:40px !important;}
.titleCss:hover{background-color:#dce1e4;}
.tableCss a{ text-decoration:none; color:#0066CC;}
.tableCss a:hover{ text-decoration:underline;}
.searchItemsCssss{ min-width:700px; overflow:auto; padding:8px; border:#bac2c5 1px solid; border-radius:4px; margin-top:5px; font-family:"宋体";}
.searchItemsCssss div{ float:left; width:22%; margin-left:10px; margin-top:20px;color:#33302f; font-family: '黑体'; font-size: 13px;}
.inputItemsCssss{ clear:both; border:#b0c6cd 1px solid; padding:15px; margin-top:5px; border-radius:4px; font-family:"宋体"; font-size:13px;}
.inputItemsCssss div{ margin-top:10px; color:#535758;  padding-bottom:8px; padding-left:15px;}

.txt{ width:200px; height:25px;border-radius:2px; border:none; border:#999999 1px solid; padding-left:10px;}
.txt:hover{ border:#841206 1px solid;}
.pwTxtCss{ border-radius:6px; width:280px; background-color:#f0f0f0; border:none; font-family:"微软雅黑"; font-size:14px; height:38px; line-height:38px; vertical-align:middle; padding-left:15px;}
.pwTxtCss:focus{outline:none; background-color:#ffdfdf;}
.txtUploadCss{ border-radius:6px; width:280px; background-color:#f0f0f0; border:none; font-family:"微软雅黑"; font-size:14px; height:38px; line-height:38px; vertical-align:middle; padding-left:15px;}
.txtUploadCss:focus{outline:none; background-color:#ffdfdf;}



.divInitOne{ border-bottom:#fbfcfc 1px solid; overflow:auto;margin-top:0px; margin-top: 20px; margin-bottom: 10px;}
.divInitTwo{ background:url(../images/two.png) #fbfcfc 3px 5px no-repeat;font-family: '黑体'; font-size: 16px;  height:30px; padding-left:15px; border-radius:4px 4px 0px 0px; padding-top:13px; min-width:100px; width:auto; font-weight:bold; color:#0ebae5; float:left; padding-right:15px;}
.inputTextCss{ width:105px; height:25px;border-radius:2px; border:none; border:#999999 1px solid; padding-left:10px;}
.inputTextCss:hover{ border:#841206 1px solid;}

.nextPageClass{ margin-top:20px;}
.nextPageClass tr{ line-height:21px;}
.nextPageClass td{ padding-left:0px; border-radius: 3px;border:#bbbebf 1px solid; background-color:#e5e7e8;width:35px; cursor:pointer; text-align:center; color:#990000 !important;
}
.nextPageClass td:hover{ background-color:#d4d6d7;}
.nextPageClass a{ color:#333333; text-decoration:none;}
.nextPageClass a:hover{ color:#CC0000;}

.searchBtnCss{ background-color:#1a9cda; letter-spacing:4px; border-radius:5px; width:110px; height:40px; line-height:40px; vertical-align:middle; cursor:pointer; border:none; color:#FFFFFF; font-family:"黑体"; font-size:14px; text-align:center;}
.searchBtnCss:hover{ background-color:#1ea9eb;}

.addItemsRecord{ background-color:#d85224; letter-spacing:4px; border-radius:5px; width:110px; height:40px; line-height:40px; vertical-align:middle; cursor:pointer; border:none; color:#FFFFFF; font-family:"黑体"; font-size:14px; text-align:center;}
.addItemsRecord:hover{ background-color:#ea5f2f;}
.saveBtnCss{ background-color:#48668e; letter-spacing:4px; border-radius:5px; width:110px; height:40px; line-height:40px; vertical-align:middle; cursor:pointer; border:none; color:#FFFFFF; font-family:"黑体"; font-size:14px; text-align:center;}
.saveBtnCss:hover{ background-color:#5a7dad;}
.handle_result_item{margin: 20px; border-radius: 5px;background-color: #ffffff; padding: 10px; color: #696464; font-family:sans-serif; }
.divInitTwo a{ text-decoration: none; color:red; font-size: 13px;}
.novel_title_click{cursor: pointer; color:#0f7ec4 }
.novel_title_click:hover{color: #c11d11; text-decoration: underline;}
.free_chapter{ background:url(../../images/free.png) no-repeat 1px 14px; padding-left:38px !important;}
.vip_chapter{ background:url(../../images/vip.png) no-repeat 1px 14px;padding-left:38px !important;}
.goodsDiv{ padding:4px; background-color:#FFFFFF; overflow:auto;}
.goodsItems{ float:left; border:#e4f6fa 1px solid; border-radius:5px; padding:5px; text-align:center; margin-left:20px; margin-top:35px; width:180px; font-family:"微软雅黑"; font-size:12px;}
.goodsItems div{ margin-top:10px;}
.goodsDetail input{ width:70px !important; height:35px !important; line-height:35px !important;} 
.goodsChangeCss{ background-color:#d85224; letter-spacing:4px; border-radius:5px; width:110px; height:40px; line-height:40px; vertical-align:middle; cursor:pointer; border:none; color:#FFFFFF; font-family:"黑体"; font-size:14px; text-align:center;}
.goodsChangeCss:hover{ background-color:#ea5f2f;}
.buttonBtnCss{ background-color:#48668e; letter-spacing:4px; border-radius:5px; width:110px; height:40px; line-height:40px; vertical-align:middle; cursor:pointer; border:none; color:#FFFFFF; font-family:"黑体"; font-size:14px; text-align:center;}
.buttonBtnCss:hover{ background-color:#5a7dad;}
.handle_result_item{font-size: 13px; font-family:Arial, Helvetica, sans-serif;}
.look_result_hand{ border-radius:4px;width:130px; background:url(../images/h_r.png) no-repeat #1a9cda 9px 11px;}
.look_result_hand a{color: #ffffff; font-size: 13px; font-family:Arial, Helvetica, sans-serif;text-decoration: none; letter-spacing: 3px;}
.look_result_hand a:hover{text-decoration: underline;}
.passOK{background:url(../images/pass_ok.png) no-repeat 4px 7px; padding-left: 32px; border-bottom: #cfcece 1px dashed;}
.passNo{background:url(../images/pass_no.png) no-repeat 4px 7px; padding-left: 32px; border-bottom: #cfcece 1px dashed;}
.hand_title_css{ font-weight:bold;color: #000000; padding-right: 10px;}
.hand_time_csss{font-family: '宋体'; font-size: 14px; margin-left: 350px; color: #aeadad;}
.web_t_css div{ line-height: 30px;}
.need_do_nums_class{ font-family:"新宋体"; background-image: url(../images/oval.png);padding:5px;font-size:10px;color:#ffffff; letter-spacing:0px;background-position:0px 3px; margin-left: 5px;background-repeat: no-repeat;}

#novelTanleDiv{margin-left: 65px;border: #fbece6 1px solid;display:none; padding: 10px;margin-top: 3px;}
#novelTanleDiv tr{line-height: 35px;}
#novelTanleDiv ul{padding: 0px; margin: 0px; list-style: none;}
#novelTanleDiv li{float: left;margin-left: 10px;}
.resetBtnCss{ background-color:#d85224; letter-spacing:4px; border-radius:5px; width:110px; height:40px; line-height:40px; vertical-align:middle; cursor:pointer; border:none; color:#FFFFFF; font-family:"黑体"; font-size:14px; text-align:center;}
.resetBtnCss:hover{ background-color:#ea5f2f;}

.qi_label_css{ background-color:#fff !important;font-size:14px; font-family:"黑体";}
.ou_label_css{ background-color:#e4e4e4 !important;font-size:14px; font-family:"黑体";}





