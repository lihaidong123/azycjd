package com.anzhi.web.pojo;

public class AnZhiFuLi {
	//2--全勤奖  0--上架福利 1-完结奖 4-精英奖
	private int novelId;
	private int fuliType;
	private int memberId;
	private String vote;
	private int successInt;
	
	public int getNovelId() {
		return novelId;
	}
	public void setNovelId(int novelId) {
		this.novelId = novelId;
	}
	public int getFuliType() {
		return fuliType;
	}
	public void setFuliType(int fuliType) {
		this.fuliType = fuliType;
	}
	public String getVote() {
		if(vote==null){
			vote="";
		}
		return vote;
	}
	public void setVote(String vote) {
		this.vote = vote;
	}
	public int getMemberId() {
		return memberId;
	}
	public void setMemberId(int memberId) {
		this.memberId = memberId;
	}
	public int getSuccessInt() {
		return successInt;
	}
	public void setSuccessInt(int successInt) {
		this.successInt = successInt;
	}
	
}
